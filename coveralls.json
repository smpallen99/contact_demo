{
  "default_stop_words": [
    "defmodule",
    "defrecord",
    "defimpl",
    "def.+(.+\/\/.+).+do"
  ],

  "custom_stop_words": [
  ],

  "coverage_options": {
    "treat_no_relevant_lines_as_covered": true,
    "minimum_coverage": 72
  },

  "skip_files": [
    "lib/contact_demo.ex",
    "lib/contact_demo/authentication.ex",
    "lib/contact_demo/authorization.ex",
    "lib/contact_demo/endpoint.ex",
    "lib/contact_demo/repo.ex",
    "test/*",
    "web/admin/*",
    "web/emails/coherence/coherence_mailer.ex",
    "web/gettext.ex",
    "web/router.ex",
    "web/views/*",
    "web/views/layout_view.ex",
    "web/web.ex",
  ]
}
